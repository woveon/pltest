
# --------------------------------------------------------------------- 
# WoveonLogger: Aspect Logging
# --------------------------------------------------------------------- 
# WOV_LOGGER_ASPECTS=
# --- should set on your own


# --------------------------------------------------------------------- 
# APIREST and APP
# --------------------------------------------------------------------- 
WOV_api_ver=\{{apirest.ver}}
WOV_api_fullurl=\{{apirest.{{STAGE}}.urlscheme}}://\{{apirest.{{STAGE}}.url}}
WOV_wwwapp_fullurl=\{{www.{{STAGE}}.app.urlscheme}}://\{{www.{{STAGE}}.app.url}}/


# --------------------------------------------------------------------- 
# Constants in WoveonEngine
# --------------------------------------------------------------------- 
WOV_we_wl_port={{we.wl_port}}
WOV_we_rl_port={{we.rl_port}}
WOV_we_ov_port={{we.ov_port}}
#WOV_we_db_user={{we.db.user}}
#WOV_we_db_collection={{we.db.collection}}
WOV_we_outofdateseconds=\{{we.{{STAGE}}.outofdateseconds}}
WOV_testuser_id={{testuser.id}}
WOV_testuser_name={{testuser.name}}
WOV_testuser_email={{testuser.email}}

# --------------------------------------------------------------------- 
# WoveonEngine Database
# --------------------------------------------------------------------- 
WOV_{{PROJECT}}db_type=mongo
WOV_{{PROJECT}}db_database=\{{we.db.{{STAGE}}.collection}}
WOV_{{PROJECT}}db_username=\{{we.db.user}}
WOV_{{PROJECT}}db_host=\{{we.db.{{STAGE}}.db_url}}
WOV_plugin_db_fullurl=mongodb://\{{we.db.{{STAGE}}.db_url}}/\{{we.db.{{STAGE}}.collection}}


# --------------------------------------------------------------------- 
# Plugin Variables
# --------------------------------------------------------------------- 
WOV_plugin_id=\{{{{PROJECT}}.id}}
WOV_plugin_name=\{{PROJECT}}
WOV_plugin_title="\{{PROJECTTITLE}}"
WOV_plugin_contactemail=\{{USEREMAIL}}
WOV_plugin_desc="\{{PROJECTDESCRIPTION}}"
WOV_plugin_inst="\{{{{PROJECT}}.instructions}}"
WOV_plugin_url={{PROJECT}}-{{STAGE}}.woveon.com
WOV_plugin_urlscheme=\{{{{PROJECT}}.{{STAGE}}.urlscheme}}


